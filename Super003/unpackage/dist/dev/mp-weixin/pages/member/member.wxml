<view><view hidden="{{!(!isLogin)}}" class="notLoggedIn login-animation"><view class="logo-img"><image class="canvas _img" src="{{$root.m0}}"></image></view><view class="prompt-information"><view class="prompt">SuperBot Ai</view><view class="prompt-detail">Welcome to SuperBot</view><view data-event-opts="{{[['tap',[['login',['$event']]]]]}}" class="login" bindtap="__e">微信授权登录</view></view></view><button class="contact" open-type="contact"></button><button class="share" open-type="share" data-event-opts="{{[['tap',[['shareFriends',['$event']]]]]}}" bindtap="__e"></button><view hidden="{{!(isLogin)}}" class="loggedIn load-animation"><view class="vertical"><view class="user-radius"><image class="original" src="/static/images/img/user.png" data-event-opts="{{[['tap',[['copyOpenId',['$event']]]]]}}" bindtap="__e"></image></view><view class="user_information"><view class="user_name">你好 人类</view><view class="user_times">{{"剩余次数 "+user.frequency+"次"}}</view></view></view><view class="receive"><view data-event-opts="{{[['tap',[['watchVideo',['$event']]]]]}}" class="receive-btn" bindtap="__e"><image class="receive-img" src="/static/images/img/signIn.png"></image><view class="receive-text"><view class="receive-0">观看视频</view><view class="receive-1">领取2次使用次数</view></view></view><view class="receive-btn"><image class="receive-img" src="/static/images/img/share.png"></image><view class="receive-text"><view class="receive-0">分享好友</view><view class="receive-1">每天1次使用次数</view></view></view></view><view class="panel"><view class="panel-han"><view>联系客服</view><image class="panel-key" src="/static/images/img/keyboard.png"></image></view><view data-event-opts="{{[['tap',[['toPrivacy',['$event']]]]]}}" class="panel-han" bindtap="__e"><view>关于隐私</view><image class="panel-key" src="/static/images/img/keyboard.png"></image></view><view data-event-opts="{{[['tap',[['toExchange',['$event']]]]]}}" class="panel-han" bindtap="__e"><view>兑换次数</view><image class="panel-key" src="/static/images/img/keyboard.png"></image></view><view data-event-opts="{{[['tap',[['toServer',['$event']]]]]}}" class="panel-han" bindtap="__e"><view>访问控制台</view><image class="panel-key" src="/static/images/img/keyboard.png"></image></view><view data-event-opts="{{[['tap',[['toConcerning',['$event']]]]]}}" class="panel-han" bindtap="__e"><view>关于小程序</view><image class="panel-key" src="/static/images/img/keyboard.png"></image></view></view><view data-event-opts="{{[['tap',[['loginOut',['$event']]]]]}}" class="exit-login" bindtap="__e">退出登录</view></view></view>